  <body class="bg-muted">
    <div class="container">
      <form method="POST" action="/clubs/create">
        <div class="form-group"></div>
          <label for="">Club Number</label>
          <input type="text" class="form-control" name="no" id="" /><br />
        <div class="form-group">
          <label for="">Club Name</label>
          <input type="text" class="form-control" name="name" id="" /><br />
        </div>
        <div class="form-group">
          <label for="">Club Description</label>
          <input type="text" class="form-control" name="description" id="" /><br />
        </div>
        <div class="form-group">
          <label for="">Department Number</label>
          <input type="text" class="form-control" name="deptNo" id="" /><br />
        </div>
        <div class="form-group">
          <label for="">Lead USN</label>
          <input type="text" class="form-control" name="leadUSN" id="" /><br />
        </div>
        <input type="submit" class="btn btn-primary"/>
      </form>
    </div>
